{
  "name": "lisp",
  "version": "0.6.1",
  "description": "simplest lisp implementation",
  "main": "lib/lisp.js",
  "scripts": {
    "test": "tape 'lib/**/*.spec.js'",
    "coverage": "nyc npm test",
    "build": "redrun clean 6to5 legacy",
    "6to5": "babel lib -d legacy",
    "legacy": "echo \"module.exports = require('./lisp');\" > legacy/index.js",
    "wisdom": "npm run build",
    "lint": "eslint lib test",
    "clean": "rimraf legacy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/lisp.git"
  },
  "keywords": [
    "lisp",
    "scheme",
    "functional"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/lisp/issues"
  },
  "homepage": "https://github.com/coderaiser/lisp",
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.0.0",
    "nyc": "^13.0.1",
    "redrun": "^6.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^6.0.0",
    "sinon-called-with-diff": "^2.0.0",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "currify": "^2.0.0",
    "squad": "^3.0.0"
  }
}
